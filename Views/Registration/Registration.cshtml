
@{
    ViewBag.Title = "Registration";
}
@model List<LabTaskRegistration.DB.CourseStudent>
<h2>Registration</h2>

<form action="" method="post">
    <ul>
        @foreach (var item in Model)
        {
            <li><input type="checkbox" name="courses[]" value="@item.id"> @item.Cours.Name </li>
        }
    </ul>
    <input type="submit" value="register" />
</form>